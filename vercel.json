{
  "functions": {
    "api/jobs.ts": {
      "maxDuration": 10
    },
    "api/jobs/[id].ts": {
      "maxDuration": 10
    },
    "api/candidates.ts": {
      "maxDuration": 10
    },
    "api/assessments/[jobId].ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
